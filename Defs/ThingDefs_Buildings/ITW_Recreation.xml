<Defs>

    <ThingDef ParentName="FurnitureBase" Name="ITW_JoyBase" Abstract="True">
        <altitudeLayer>Building</altitudeLayer>
        <rotatable>false</rotatable>
        <socialPropernessMatters>true</socialPropernessMatters>
        <fillPercent>0.40</fillPercent>
        <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
        <size>(2,2)</size>
        <building>
            <joyKind>Gaming_Cerebral</joyKind>
        </building>
        <statBases>
            <WorkToBuild>12000</WorkToBuild>
            <Mass>5</Mass>
            <Flammability>0</Flammability>
            <MaxHitPoints>250</MaxHitPoints>
            <Beauty>12</Beauty>
            <JoyGainFactor>1.5</JoyGainFactor>
        </statBases>
        <costList>
            <ITW_MaterialCommon>50</ITW_MaterialCommon>
            <ITW_Component>5</ITW_Component>
        </costList>
        <comps>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <basePowerConsumption>150</basePowerConsumption>
                <shortCircuitInRain>false</shortCircuitInRain>
            </li>
            <li Class="CompProperties_Flickable" />
        </comps>
        <researchPrerequisites>
            <li>ITW_Recreation</li>
        </researchPrerequisites>
        <pathCost>42</pathCost>
        <passability>PassThroughOnly</passability>
        <designationCategory>Joy</designationCategory>
        <canOverlapZones>false</canOverlapZones>
        <constructionSkillPrerequisite>10</constructionSkillPrerequisite>
        <tradeTags>
            <li>PhitechRecreationBuilding</li>
        </tradeTags>
    </ThingDef>

    <JoyGiverDef Name="ITW_JoyGiverBase" Abstract="True">
        <giverClass>JoyGiver_InteractBuildingSitAdjacent</giverClass>
        <baseChance>5</baseChance>
    </JoyGiverDef>

    <JobDef Name="ITW_JoyJobBase" Abstract="True">
        <driverClass>JobDriver_SitFacingBuilding</driverClass>
        <reportString>playing a computer game.</reportString>
        <joyXpPerTick>0.01</joyXpPerTick>
        <joyDuration>4000</joyDuration>
        <joyMaxParticipants>8</joyMaxParticipants>
        <taleOnCompletion>PlayedGame</taleOnCompletion>
        <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
    </JobDef>


