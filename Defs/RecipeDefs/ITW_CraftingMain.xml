    <!-- Base Resources -->

    <RecipeDef ParentName="ITW_CraftingMainRecipeBase">
		<defName>ITW_MaterialCommon_OneBatch</defName>
		<label>synthesize common starsteel</label>
		<description>Assemble, infuse, and edit a batch of common starsteel. This requires a versatile base, a noble metal for stability, and a dense metal for infusion density.</description>
		<ingredients>
			<li><filter><thingDefs><li>Plasteel</li></thingDefs></filter><count>7</count></li>
			<li><filter><thingDefs><li>Gold</li></thingDefs></filter><count>1</count></li>
			<li><filter><thingDefs><li>Uranium</li></thingDefs></filter><count>2</count></li>
			<li><filter><thingDefs><li>ITW_PhiMatter</li></thingDefs></filter><count>100</count></li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Plasteel</li>
				<li>Gold</li>
				<li>Uranium</li>
				<li>ITW_PhiMatter</li>
			</thingDefs>
		</fixedIngredientFilter>
		<workAmount>600</workAmount>
		<products>
            <ITW_MaterialCommon>10</ITW_MaterialCommon>
        </products>
        <unfinishedThingDef>ITW_UnfinishedMaterial</unfinishedThingDef>
		<researchPrerequisite>ITW_Starsteel</researchPrerequisite>
		<jobString>Synthesizing starsteel.</jobString>
    </RecipeDef>

