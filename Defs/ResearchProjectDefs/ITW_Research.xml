    <!-- BASE RESEARCHES -->

    <ResearchProjectDef Name="ITW_ResearchBase" Abstract="True">
        <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
        <requiredResearchFacilities>
            <li>MultiAnalyzer</li>
            <li>ITW_ResearchUnlocker</li>
        </requiredResearchFacilities>
		<techLevel>Archotech</techLevel>
        <tab>ITW</tab>
    </ResearchProjectDef>
    

    <ResearchProjectDef ParentName="ITW_ResearchBase">
        <defName>ITW_Initial</defName>
        <label>phi-matter principles</label>
        <description>Work and experiment with phi-matter with the help of a machine persona.</description>
        <baseCost>10000</baseCost>
		<techLevel>Ultra</techLevel>
        <requiredResearchFacilities Inherit="False">
            <li>MultiAnalyzer</li>
        </requiredResearchFacilities>
		<prerequisites>
			<li>ShipComputerCore</li>
		</prerequisites>
        <researchViewX>0</researchViewX>
        <researchViewY>2.5</researchViewY>
    </ResearchProjectDef>

    <!-- Considering mandatory linkables, multiply desired baseCost past here by 1.5x -->

    <ResearchProjectDef ParentName="ITW_ResearchBase">
        <defName>ITW_Starsteel</defName>
        <label>starsteel fabrication</label>
        <description>Produce and work with starsteel.</description>
        <baseCost>22500</baseCost> <!-- effective 15000 -->
		<prerequisites>
			<li>ITW_Initial</li>
		</prerequisites>
        <researchViewX>1</researchViewX>
        <researchViewY>2.5</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef ParentName="ITW_ResearchBase">
        <defName>ITW_PowerFueled</defName>
